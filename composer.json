{
    "name": "EpicArrow/phoenix_music",
    "description": "Phoenix Music",
    "keywords": [
        "portfolio",
        "phoenix music",
        "posts",
        "publishing",
        "platform",
        "laravel",
        "markdown",
        "developers"
    ],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Ferdinand Frank",
            "email": "ferdinand-frank@web.de"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.20",
        "doctrine/dbal": "^2.5",
        "laravel/scout": "^1.1",
        "maatwebsite/excel": "~2.1.0",
        "talvbansal/media-manager": "^2.0",
        "teamtnt/laravel-scout-tntsearch-driver": "^1.1",
        "barryvdh/laravel-ide-helper": "^2.2",
        "martinlindhe/laravel-vue-i18n-generator": "^0.1.7",
        "guzzlehttp/guzzle": "^6.2",
        "intervention/image": "^2.3"
    },
    "require-dev": {
        "erikgall/eloquent-phpunit": "~1.0",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/dom-crawler": "~3.0",
        "symfony/css-selector": "~3.0",
        "fzaninotto/faker": "^1.6"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}